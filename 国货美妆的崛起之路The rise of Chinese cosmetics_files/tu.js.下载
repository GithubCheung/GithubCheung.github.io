                    return data1[id].slice(0,20)+'<br>'+data1[id].slice(20,40)+'<br>'+data1[id].slice(40,60)+'<br>'+data1[id].slice(60);
                }; 
            },
        },
        xAxis: {
            type: 'category',
            name: '城市',
            axisLabel: {
                interval:0,
                rotate:0,
            },

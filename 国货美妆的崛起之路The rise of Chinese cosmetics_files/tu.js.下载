function tu2(){
    var myChart2 = echarts.init(document.getElementById('main2'));    
    var geoCoordMap = {
                '北京': [116.413387, 39.910924],
                '上海': [121.48054, 31.235929],
                '广州': [113.271429, 23.135336],
                '深圳': [114.064552, 22.548456],
                '武汉': [114.311586, 30.598467],
                '成都': [104.08153, 30.655823],
                '重庆': [106.558437, 29.568996],
                '南京': [118.802422, 32.064652],
                '天津': [117.20952, 39.093669],
                '西安': [108.946465, 34.347269],
                '杭州': [120.21551, 30.253082],
                '昆明': [102.852451, 24.873998],
                '宁波': [121.628572, 29.866034],
                '郑州': [113.631416, 34.75344],
                '沈阳': [123.455986, 41.720915],
                '南宁': [108.373454, 22.822607],
                '合肥': [117.233441, 31.826578],
                '大连': [121.621628, 38.918953],
                '长沙': [112.94547, 28.23489],
                '南昌': [115.864585, 28.689455],
                '青岛': [120.389458, 36.072227],
                '长春': [125.330606, 43.821954],
                '贵阳': [106.63658, 26.653324],
                '厦门': [118.096435, 24.485408],
                '石家庄': [114.521529, 38.048312],
                '乌鲁木齐': [87.624438, 43.830763],
                '福州': [119.30347, 26.080429],
                '哈尔滨': [126.541611, 45.808824]
        };

    var data = [{name: '北京',value: 90213},{name: '上海',value: 89329},{name: '广州',value: 74867},{name: '深圳',value: 44477},{name: '成都',value: 32343},{name: '南京',value: 28149},{name: '武汉',value: 25552},{name: '重庆',value: 23715},{name: '西安',value: 22346},{name: '杭州',value: 13459},{name: '天津',value: 11718},{name: '沈阳',value: 7374},{name: '郑州',value: 7402},{name: '长沙',value: 5927},{name: '南宁',value: 5408},{name: '昆明',value: 5022},{name: '大连',value: 4803},{name: '合肥',value: 4148},{name: '青岛',value: 3946},{name: '长春',value: 4100},{name: '南昌',value: 3424},{name: '宁波',value: 3086},{name: '石家庄',value: 2313},{name: '哈尔滨',value: 2287},{name: '福州',value: 1532},{name: '厦门',value: 1198},{name: '贵阳',value: 1092},{name: '乌鲁木齐',value: 242}];

    var convertData = function (data) {
        var res = [];
        for (var i = 0; i < data.length; i++) {
            var geoCoord = geoCoordMap[data[i].name];
            if (geoCoord) {
                res.push({
                    name: data[i].name,
                    value: geoCoord.concat(data[i].value/200)
                });
            }
        }
        return res;
    };

    var convertedData = [
        convertData(data),
        convertData(data.sort(function (a, b) {
            return b.value - a.value;
        }).slice(0, 6))
    ];


    option = {
        backgroundColor: '#404a59',
        animation: true,
        animationDuration: 1000,
        animationEasing: 'cubicInOut',
        animationDurationUpdate: 1000,
        animationEasingUpdate: 'cubicInOut',
        title: [
            {
                text: '2019年一季度我国中心城市客流状况地理分布图',
                subtext: '（可在地图上框选城市，鼠标滚轮可控制地图大小，右上角工具箱有更多可选项）',
                left: 'center',
                textStyle: {
                    color: '#fff'
                }
            },
            {
                id: 'statistic',
                right: 120,
                top: 40,
                width: 100,
                textStyle: {
                    color: '#fff',
                    fontSize: 16
                }
            }
        ],
        toolbox: {
            iconStyle: {
                normal: {
                    borderColor: '#fff'
                },
                emphasis: {
                    borderColor: '#b1e4ff'
                }
            },
            feature: {
                dataZoom: {},
                brush: {
                    type: ['rect', 'polygon', 'clear']
                },
                saveAsImage : {show: true}
            }
        },
        brush: {
            outOfBrush: {
                color: '#abc'
            },
            brushStyle: {
                borderWidth: 2,
                color: 'rgba(0,0,0,0.2)',
                borderColor: 'rgba(0,0,0,0.5)',
            },
            seriesIndex: [0, 1],
            throttleType: 'debounce',
            throttleDelay: 300,
            geoIndex: 0
        },
        geo: {
            map: 'china',
            left: '10',
            right: '35%',
            top:'25%',
            center: [117.98561551896913, 31.205000490896193],
            zoom: 1.5,
            label: {
                emphasis: {
                    show: false
                }
            },
            roam: true,
            itemStyle: {
                normal: {
                    areaColor: '#323c48',
                    borderColor: '#111'
                },
                emphasis: {
                    areaColor: '#2a333d'
                }
            }
        },
        tooltip: {
            formatter: function (params) {
                    var id = params.dataIndex;
                    var keliu = data[id].value/100;
                    var chengshi =data[id].name;
                    return  chengshi+'<br>客流：'+keliu;
            },
        },
        grid: {
            right: 40,
            top: 100,
            bottom: 40,
            width: '30%'
        },
        xAxis: {
            type: 'value',
            scale: true,
            position: 'top',
            boundaryGap: false,
            splitLine: {show: false},
            axisLine: {show: true,lineStyle: {color: '#ddd'}},
            axisTick: {show: true,lineStyle: {color: '#ddd'}},
            axisLabel: {margin: 2, textStyle: {color: '#aaa'}},
        },
        yAxis: {
            type: 'category',
            name: '单位：百万人次',
            nameGap: 16,
            axisLine: {show: true, lineStyle: {color: '#ddd'}},
            axisTick: {show: true, lineStyle: {color: '#ddd'}},
            axisLabel: {interval: 0, textStyle: {color: '#ddd'}},
            data: []
        },
        series : [
            {
                name: '客流',
                type: 'scatter',
                coordinateSystem: 'geo',
                data: convertedData[0],
                symbolSize: function (val) {
                    return Math.max(val[2] / 10, 8);
                },
                label: {
                    normal: {
                        formatter: '{b}',
                        position: 'right',
                        show: true
                    },
                    emphasis: {
                        show: true
                    }
                },
                itemStyle: {
                    normal: {
                        color: '#ddb926'
                    }
                }
            },
            {
                name: 'Top 5',
                type: 'effectScatter',
                coordinateSystem: 'geo',
                data: convertedData[1],
                symbolSize: function (val) {
                    return Math.max(val[2] / 10, 8);
                },
                showEffectOn: 'emphasis',
                rippleEffect: {
                    brushType: 'stroke'
                },
                hoverAnimation: true,
                label: {
                    normal: {
                        formatter: '{b}',
                        position: 'right',
                        show: true
                    }
                },
                itemStyle: {
                    normal: {
                        color: '#f4e925',
                        shadowBlur: 10,
                        shadowColor: '#333'
                    }
                },
                zlevel: 1
            },
            {
                id: 'bar',
                zlevel: 2,
                type: 'bar',
                symbol: 'none',
                itemStyle: {
                    normal: {
                        color: '#ddb926'
                    }
                },
                data: []
            }
        ]
    };


    myChart2.on('brushselected', renderBrushed);

    setTimeout(function () {
        myChart2.dispatchAction({
            type: 'brush',
            areas: [
                {
                    geoIndex: 0,
                    brushType: 'polygon',
                    coordRange: [[102.23,27.88],[102.23,36.62],[115.47,36.62],[115.47,27.88]]
                }
            ]
        });
    }, 0);


    function renderBrushed(params) {
        var mainSeries = params.batch[0].selected[0];

        var selectedItems = [];
        var categoryData = [];
        var barData = [];
        var maxBar = 30;
        var sum = 0;
        var count = 0;

        for (var i = 0; i < mainSeries.dataIndex.length; i++) {
            var rawIndex = mainSeries.dataIndex[i];
            var dataItem = convertedData[0][rawIndex];
            var pmValue = dataItem.value[2];

            sum += pmValue;
            count++;

            selectedItems.push(dataItem);
        }

        selectedItems.sort(function (a, b) {
            return a.value[2] - b.value[2];
        });

        for (var i = 0; i < Math.min(selectedItems.length, maxBar); i++) {
            categoryData.push(selectedItems[i].name);
            barData.push(selectedItems[i].value[2]*2);
        }
        this.setOption({
            yAxis: {
                data: categoryData
            },
            xAxis: {
                axisLabel: {show: !!count}
            },
            title: {
                id: 'statistic',
                text: '被框选城市客流状况',
                right: 40,
                top:'10%',
            },
            series: {
                id: 'bar',
                data: barData,
                tooltip: {
                    formatter: function (params) {
                            var id = params.dataIndex;
                            var keliu = barData[id];
                            var chengshi = categoryData[id];
                            return  chengshi+'<br>客流：'+keliu;
                    },
                },
            }
        });
    }
    myChart2.setOption(option);
    window.addEventListener("resize", function () {
        myChart2.resize();
    }); 
}
tu2();



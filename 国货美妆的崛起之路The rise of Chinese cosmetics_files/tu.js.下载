    {name: '地铁APP', value: 3},
    {name: '检测人流', value: 4},
    {name: '轨道交通', value: 8},
    {name: '互联互通', value: 19},
    {name: '运行效率', value: 2},
    {name: '地铁营收', value: 7},
    {name: '政府投资', value: 3},
    {name: '刷脸支付', value: 18},
    {name: '客流增加', value: 2},
    {name: '票价涨幅', value: 3},
    {name: '移动支付时代', value: 2},
    {name: '无线网络', value: 5},
    {name: 'AFC系统', value: 2},
    {name: '支付宝乘车码', value: 4},
    {name: '城市地铁', value: 3},
    {name: '互联网+', value: 3},
    {name: '飞速发展', value: 2},
    {name: '互联网购票', value: 6},
    {name: '建设规划', value: 2},
    {name: '机场线', value: 1},
    {name: '云支付', value: 9},
    {name: '无人驾驶', value: 20}
];

var wordcloud_car_color =["#60ACFC", "#61D7D3", "#AEE498", "#32D3EB", "#999999"];
option = {
    series: [ {
        type: 'wordCloud',
        sizeRange: [10, 80],
        rotationRange: [-90, 90],
        rotationStep: 90,
        gridSize: 2,
        shape: 'square',
        left: 'center',
        top: 'center',
        drawOutOfBound: false,
        textStyle: {
            normal: {
                color: function () {
                  var index = Math.floor((Math.random()*wordcloud_car_color.length));
                  return wordcloud_car_color[index];
                }
        },
            emphasis: {
                color: '#60ACFC',
                shadowBlur: 6,
                shadowColor: '#dddddd'
            }
        },
        data: keywords.sort(function (a, b) {
            return b.value  - a.value;
        })
    } ]
};
    myChart12.setOption(option);
    window.addEventListener("resize", function () {
        myChart12.resize();
    }); 
}


            y:'top',
            textStyle:{
                color:'white'
            }
        },
        tooltip: {
            formatter: function(params) {
                id = params.dataIndex;
                if (data1[id].length<20){
                    return data1[id];
                }else if(20<data1[id].length&&data1[id].length<40){
                    return data1[id].slice(0,20)+'<br>'+data1[id].slice(20,40);
                }else if(40<data1[id].length&&data1[id].length<60){
                    return data1[id].slice(0,20)+'<br>'+data1[id].slice(20,40)+'<br>'+data1[id].slice(40,60);

function tu7(){
    var myChart7 = echarts.init(document.getElementById('main7'));
    var spNum = 5,_max=34;
    var legendData = ['2015新增城市数', '2016新增城市数', '2017新增城市数','2018新增城市数','2019新增城市数'];
    var y_data = ['2015', '2016', '2017', '2018', '2019'];
    var _datamax = [32,32,32,32,32,32],
        _data1 = [3,3,3,3,3],
        _data2 = [null,8,8,8,8],
        _data3 = [null,null,8,8,8];
        _data4 = [null,null,null,8,8];
        _data5 = [null,null,null,null,5];
        _data6 = [null,null,null,null,null];
    var chengshi = [['长沙', '深圳', '广州'],['上海', '天津', '西安', '苏州', '郑州', '杭州', '宁波', '青岛'],['无锡', '福州', '北京', '武汉', '南京', '成都', '南宁', '厦门'],['沈阳', '长春', '南昌', '贵阳', '大连', '东莞', '合肥', '乌鲁木齐'],['重庆', '昆明', '哈尔滨', '石家庄', '温州']];
    var fomatter_fn = function(v) {
        return v.value
    }
    var _label = {
        normal: {
            show: true,
            position: 'inside',
            formatter: fomatter_fn,
            textStyle: {
                color: 'white',
                fontSize: 16
            }
        }
    };
    option = {
        title: {
            text: '我国各城市开通互联网购票或扫码购票的启用时间',

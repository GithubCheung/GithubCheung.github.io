function tu5(){
    var myChart5 = echarts.init(document.getElementById('main5'));
    var charts = {
        unit: '单位：万人次',
        names: ['2019', '2018'],
        lineX: ['武汉', '成都', '上海', '南京', '西安', '重庆', '杭州', '福州', '广州', '苏州', '长沙', '青岛', '南昌', '合肥', '北京', '南宁', '深圳', '昆明', '宁波', '郑州', '沈阳'],
        value: [
            [397.86, 371.73, 885.63, 366.08, 306.49, 293.73, 196.6, 46.09, 940.63, 133.98, 102.71, 74.31, 63.39, 62.31, 859.11, 86.16, 513.65, 65.98, 45.75, 99.95, 87.4],
            [315.63, 297.8, 814.6, 309.27, 251.66, 256.33, 163.84, 17.56, 919.23, 114.13, 85.93, 58.03, 51.23, 52.38, 849.54, 76.6, 504.89, 59, 40.03, 94.7, 85.46]
        ]

    }
    var color = ['rgba(23, 255, 243', 'rgba(255,100,97']
    var lineY = []

    for (var i = 0; i < charts.names.length; i++) {
        var x = i
        if (x > color.length - 1) {
            x = color.length - 1
        }
        var data = {
            name: charts.names[i],
            type: 'line',
            smooth: true,
            symbol: 'circle',
            symbolSize: 5,
            data: charts.value[i]
        }
        lineY.push(data)
    }
    var option = {
        color:['#ddb926','#69fff9'],
        backgroundColor: '#404a59',
        title: {
            text: '2018-2019热点城市五一地铁人流量对比',
            x:'center',
            y:'top',
            textStyle:{
                color:'white',
            }
        },
        tooltip: {
            trigger: 'axis'
        },
        legend: {
            data: charts.names,
            textStyle: {
                fontSize: 12,
                color: 'white'
            },
            right: '4%'
        },
        xAxis: {
            type: 'category',
            boundaryGap: false,
            data: charts.lineX,
            axisLabel: {
                textStyle: {
                    color: 'white',
                    interval:2,
                    rotate:40
                },
            },
            axisLabel: {
                interval:0,
                rotate:0,
            },
            axisLine: {
                lineStyle: {
                    color: 'white'
                }
            },
            splitLine: {
                lineStyle: {
                    color: 'white'
                }
            },
        },
        yAxis: {
            name: charts.unit,
            type: 'value',
            axisLabel: {
                formatter: '{value}',
                textStyle: {
                    color: 'white'
                }
            },
            splitLine: {
                lineStyle: {
                    color: 'white'
                }
            },
            axisLine: {
                lineStyle: {
                    color: 'white'
                }
            }
        },
        series: lineY
    }
    myChart5.setOption(option);
    window.addEventListener("resize", function () {
        myChart5.resize();
    }); 
}
tu5();

